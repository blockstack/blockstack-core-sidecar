    },
    "peer_version": {
      "type": "integer"
    },
    "burn_consensus": {
      "type": "string"
    },
    "burn_block_height": {
      "type": "integer"
    },
    "stable_burn_consensus": {
      "type": "string"
    },
    "stable_burn_block_height": {
      "type": "integer"
    },
    "server_version": {
      "type": "string"
    },
    "network_id": {
      "type": "integer"
    },
    "parent_network_id": {
      "type": "integer"
    },
    "stacks_tip_height": {
      "type": "integer"
    },
    "stacks_tip": {
      "type": "string"
    },
    "stacks_tip_burn_block": {
      "type": "string"
    },
    "exit_at_block_height": {
      "type": "integer"
    }
  }
}
