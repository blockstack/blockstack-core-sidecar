{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Block",
  "description": "A block",
  "type": "object",
  "required": ["height", "hash", "previous", "txs"],
  "properties": {
    "height": {
      "type": "integer"
    },
    "hash": {
      "type": "string"
    },
    "previous": {
      "type": "string"
    },
    "txs": {
      "type": "array",
      "items": {
        "type": "string"
      }
    }
  }
}
