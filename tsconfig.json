{
  "compilerOptions": {
    "target": "es2019",
    "module": "commonjs",
    "moduleResolution": "node",
    "declaration": false,
    "strict": true,
    "outDir": "lib",
    "sourceMap": false,
    "esModuleInterop": false,
    "allowSyntheticDefaultImports": false,
    "resolveJsonModule": true,
  },
  "include": ["src/**/*"],
  "exclude": ["lib"]
}
